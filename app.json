{
  "expo": {
    "name": "ReticleStats",
    "slug": "reticlestats",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/logo.png",
    "scheme": "reticlestats",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/brand/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.reticlestats.app",
      "supportsTablet": true,
      "icon": "./assets/brand/logo.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "updates": {
      "url": "https://u.expo.dev/7bf45c78-f2fe-4a73-9b1c-408243b55069"
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/brand/logo.png",
        "backgroundImage": "./assets/brand/logo.png",
        "monochromeImage": "./assets/brand/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.reticlestats.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/brand/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/brand/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "native",
          "organization": "reticlestats"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "92107bc7-13c1-40c0-b361-0f49f73416f2"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "ilieberacha"
  }
}
