{
  "cli": {
    "version": ">= 16.25.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "ios": {
        "simulator": true
      },
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "staging": {
      "ios": {
        "simulator": false
      },
      "developmentClient": true,
      "distribution": "internal",
      "channel": "staging"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env":{
        "SENTRY_DISABLE_AUTO_UPLOAD": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
